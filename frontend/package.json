{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev -p 3002 --turbopack",
    "build": "next build",
    "generate:og": "node scripts/generate-og-image.mjs",
    "prebuild": "node scripts/generate-og-image.mjs",
    "start": "next start",
    "lint": "biome lint ./src",
    "lint:fix": "biome lint --write ./src",
    "format": "biome format ./src",
    "format:fix": "biome format --write ./src",
    "check": "biome check ./src",
    "check:fix": "biome check --write ./src",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:coverage:ui": "vitest --ui --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "prepare": "husky",
    "type-check": "tsc --noEmit",
    "analyze": "ANALYZE=true npm run build",
    "clean": "rm -rf .next out node_modules/.cache",
    "ci": "npm run test:run && npm run build"
  },
  "overrides": {
    "undici": "^6.0.0",
    "esbuild": "^0.25.0",
    "path-to-regexp": "^8.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-primitive": "^2.1.4",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@shadcn/ui": "^0.0.4",
    "@tailwindcss/postcss": "^4.1.18",
    "@tanstack/react-query": "^5.90.20",
    "autoprefixer": "^10.4.23",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cookie": "^1.0.2",
    "framer-motion": "^12.29.2",
    "howler": "^2.2.4",
    "lucide-react": "^0.563.0",
    "motion": "^12.29.2",
    "next": "^16.1.5",
    "postcss": "^8.5.3",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-hook-form": "^7.71.1",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.7",
    "use-sound": "^5.0.0",
    "zod": "^4.3.6",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.4",
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^16.1.5",
    "@playwright/test": "^1.58.2",
    "@tanstack/react-query-devtools": "^5.91.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25.3.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.0",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "lint-staged": "^16.2.7",
    "msw": "^2.12.7",
    "npm-force-resolutions": "^0.0.10",
    "playwright": "^1.58.0",
    "sharp": "^0.34.5",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18",
    "wrangler": "^4.60.0"
  },
  "volta": {
    "node": "24.13.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "biome check --write --no-errors-on-unmatched",
      "vitest related --run --reporter=verbose"
    ],
    "*.{json,md}": [
      "biome format --write --no-errors-on-unmatched"
    ],
    "*.{css,scss}": [
      "biome format --write --no-errors-on-unmatched"
    ]
  }
}